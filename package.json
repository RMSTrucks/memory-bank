{
  "name": "vector-operations-system",
  "version": "1.0.0",
  "description": "A TypeScript system for managing vector operations with OpenAI embeddings and Pinecone vector storage",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf build",
    "test": "jest",
    "example": "node build/examples/vector-operations-example.js",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "openai",
    "pinecone",
    "embeddings",
    "vector",
    "typescript",
    "cache"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@pinecone-database/pinecone": "^1.0.0",
    "dotenv": "^16.0.3",
    "openai": "^4.0.0",
    "tiktoken": "^1.0.20"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
